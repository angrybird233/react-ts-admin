
import { lazy } from 'react';
const DynamicImport = () => lazy(() => import(`@/views/{{ pname }}/{{ ename }}.tsx`));

export default {
  path: '{{ ename }}', // （*必填）
  key: '{{ key }}', // 路由跳转name
  meta: {
    type: {{ type }}, // 1 => 菜单 2 => 视图 3 => 菜单+视图
    title: '{{ cname }}', // 菜单以及权限验证所显示的中文名 （*必填）
    menuIcon: '', // 菜单以及权限验证所显示的 icon
    description: '{{ cname }}', // 模块描述
    file_path: '{{ pname }}', // 文件路径
    showMenu: true // 是否在菜单中显示
  },
  element: () => DynamicImport()
  children: []
}
